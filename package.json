    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.6.2",
    "circle-ci": "^0.1.1",
    "gh-pages": "^3.2.3",
    "i": "^0.3.7",
    "leaflet": "^1.7.1",
    "nodemon": "^2.0.15",
    "npm": "^8.2.0",
    "pigeon-maps": "^0.19.7",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "^17.0.2",
    "react-grid-system": "^8.0.1",
    "react-icons": "^4.3.1",
    "react-leaflet": "^3.2.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^4.0.3",
